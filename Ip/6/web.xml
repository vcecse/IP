<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>example.login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>example.reg.reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profile</servlet-name>
        <servlet-class>example.profile.profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>order</servlet-name>
        <servlet-class>example.order.order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>catalog</servlet-name>
        <servlet-class>example.catalog.catalog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>order</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>catalog</servlet-name>
        <url-pattern>/catalog</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>main.html</welcome-file>
        <welcome-file>login.html</welcome-file>
        <welcome-file>reg.html</welcome-file>
        <welcome-file>profile.html</welcome-file>
        <welcome-file>catalog.html</welcome-file>
        <welcome-file>order.html</welcome-file>
    </welcome-file-list>
</web-app>